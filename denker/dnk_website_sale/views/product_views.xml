<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record model="ir.ui.view" id="dnk_product_product_website_tree_view">
      <field name="name">dnk.product.product.website.tree</field>
      <field name="model">product.product</field>
      <field name="inherit_id" ref="product.product_template_form_view"/>
      <field name="arch" type="xml">
          <field name="website_id" position="after">
              <field name="dnk_website_ids" widget="many2many_tags"/>
          </field>
      </field>
  </record>

</odoo>
