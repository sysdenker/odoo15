<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="dnk_crm_pd_kanban_view" model="ir.ui.view">
        <field name="name">dnk_crm_pd_kanban_view</field>
        <field name="model">dnk.crm.product.dev</field>
        <field name="arch" type="xml">
            <kanban create="0" default_group_by="dnk_stage_id" class="o_kanban_small_column o_kanban_dnk_crm_product_dev">
                 <field name="dnk_stage_id" options='{"group_by_tooltip": {"dnk_description": "Descripci贸n"}}' />
                 <field name="dnk_lead_id" />
                 <field name="name" />
                 <field name="dnk_family_id"/>
                 <field name="dnk_partner_id"/>
                 <field name="dnk_planned_revenue" sum="- Total"/>
                 <field name="dnk_contact_name" />
                 <field name="dnk_user_id" />
                 <field name="dnk_color" />
                 <field name="dnk_active" />
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_content oe_kanban_global_click">
                                <div class="oe_kanban_content">
                                    <div>
                                        <strong><field name="dnk_id"/>-<field name="dnk_lead_id"/></strong>
                                    </div>
                                    <div class="text-muted">
                                        <t t-if="record.dnk_planned_revenue.raw_value"><field name="dnk_planned_revenue" /><span t-if="record.dnk_partner_id.value">,</span></t> <span t-if="record.dnk_partner_id.value"> <t t-esc="record.dnk_partner_id.value"/></span>
                                    </div>
                                    <div class="text-muted">
                                        <strong class="o_kanban_record_title">- Fam :</strong><span t-if="record.dnk_family_id.raw_value"><t t-esc="record.dnk_family_id.value"/></span>
                                    </div>
                                    <div class="o_kanban_footer">
                                        <div>
                                            <img t-att-src="kanban_image('res.users', 'image_small', record.dnk_user_id.raw_value)" t-att-alt="record.dnk_user_id.value" t-att-title="record.dnk_user_id.value" width="24" height="24" class="oe_kanban_avatar pull-right"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="oe_clear"></div>
                            </div>
                        </t>
                    </templates>
                </kanban>
        </field>
    </record>

