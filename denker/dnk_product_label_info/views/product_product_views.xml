<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="dnk_product_labels_form_view" model="ir.ui.view">
        <field name="name">dnk.product.labels.form.view</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="groups_id" eval="[(4, ref('dnk_printing_labels.dnk_printing_label_user_group'))]"/>
        <field name="arch" type="xml">
            <field name="route_ids" position="before">
                <field name="dnk_ir_ui_view_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>
</odoo>
