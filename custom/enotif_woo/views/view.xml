<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="enotif_woo_assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/enotif_woo/static/src/css/main.css"/>        
            <script type="text/javascript" src="/enotif_woo/static/src/js/main.js"/>
        </xpath>        
    </template>


    <record id="enotif_woo_action" model="ir.actions.client">
        <field name="name">WooCommerce Keys</field>    
        <field name="tag">enotif-woo-action</field>        
    </record>

    <menuitem id="menu_enotif_woo_content" name="WooCommerce Keys" action="enotif_woo_action" parent="enotif.menu_enotif_root" sequence="10"/>
           
            
</odoo>