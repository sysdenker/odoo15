        <field name="model">webkul.website.addons</field>
        <field name="inherit_id" ref="website_webkul_addons.config_webkul_website_addons_form" />
        <field name="arch" type="xml">
            <xpath expr="//td[@name='module_website_stock']" position="replace">
                <td>
                    <button type="object" name="get_configuration_view" string="Configuration" class="oe_link"></button>
                </td>
            </xpath>
        </field>
    </record>

</odoo>